name: lance angular
run-name: projet de ${{ github.actor }} 
on: [push]
jobs:
  lance le projet angular:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v4
      - uses: actions/setup-node@v3
        with:
          node-version: '14'
      - run: npm install -g @angular/cli
      - run: ng serve
